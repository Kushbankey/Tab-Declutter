{
  "manifest_version": 3,
  "name": "Tab Declutter Tool",
  "version": "1.0",
  "action": {
    "default_popup": "index.html",
    "default_icon": "broom.png"
  },
  "permissions": ["tabs", "storage", "tabGroups"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "broom.png",
    "48": "broom.png",
    "128": "broom.png"
  }
}
